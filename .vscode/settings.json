{
  "breadcrumbs.enabled": true,
  "workbench.sideBar.location": "left",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.editor.enablePreview": false,
  "emmet.triggerExpansionOnTab": true,
  "editor.suggestSelection": "first",
  "editor.renderWhitespace": "boundary",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "git.suggestSmartCommit": false,
  "redhat.telemetry.enabled": true,
  "editor.codeLensFontFamily": "Monaco",
  "editor.inlayHints.fontFamily": "Monaco",
  "editor.fontFamily": "Monaco",
  "editor.fontWeight": "400",
  "editor.insertSpaces": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "files.exclude": {
    "**/__pycache__": true,
    "**/*.idea": true,
    "**/*.pyc": true
  },
  "files.associations": {
    "*Dockerfile*": "dockerfile",
    "yml": "yaml",
    "*.html": "html"
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.rvm/**": true,
    "**/node_modules/**": true,
    "**/GoogleDrive/**": true,
    "**/Dropbox/**": true,
    "**/.nuxt/**": true,
    "**/build/**": true,
    "**/Pictures/**": true
  },
  "html.format.enable": true,
  "terminal.external.osxExec": "WezTerm",
  "terminal.integrated.fontFamily": "SauceCodePro Nerd Font",
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.fontWeight": "200",
  "workbench.preferredDarkColorTheme": "Default Light+",
  "workbench.iconTheme": "material-icon-theme",
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "eslint.lintTask.enable": true,
  "python.pythonPath": "/Users/yamadaasuma/.pyenv/shims/python",
  "python.venvFolders": ["venv"],
  "python.defaultInterpreterPath": "/Users/yamadaasuma/.pyenv/shims/python",
  "python.venvPath": "/Users/a.yamada/.pyenv",
  "php-cs-fixer.executablePath": "/usr/local/bin/php-cs-fixer",
  "php-cs-fixer.onsave": true,
  "php-cs-fixer.rules": "@Symfony,-phpdoc_summary,-phpdoc_to_comment",
  "yaml.customTags": [
    "!And",
    "!And sequence",
    "!If",
    "!If sequence",
    "!Not",
    "!Not sequence",
    "!Equals",
    "!Equals sequence",
    "!Or",
    "!Or sequence",
    "!FindInMap",
    "!FindInMap sequence",
    "!Base64",
    "!Join",
    "!Join sequence",
    "!Cidr",
    "!Ref",
    "!Sub",
    "!Sub sequence",
    "!GetAtt",
    "!GetAZs",
    "!ImportValue",
    "!ImportValue sequence",
    "!Select",
    "!Select sequence",
    "!Split",
    "!Split sequence"
  ],
  "yaml.validate": false,
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[php]": {
    "editor.insertSpaces": true,
    "editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
  },
  "[markdown]": {
    "files.trimTrailingWhitespace": false
  },
  "[dockerfile]": {
    "editor.quickSuggestions": {
      "strings": true
    }
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[makefile]": {
    "editor.insertSpaces": false
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.python",
    "gitlens.codeLens.symbolScopes": ["!Module"],
    "editor.wordBasedSuggestions": "off"
  },
  "material-icon-theme.activeIconPack": "react",
  "material-icon-theme.files.associations": {
    "*.cairo": "rust"
  },
  "[cairo]": {
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.semanticHighlighting.enabled": true
  },
  "cairo1.enableLanguageServer": true,
  "cairo1.enableScarb": true,
  "cairo1.scarbPath": "~/.asdf/shims/scarb",
  "cairo1.languageServerPath": "~/.asdf/installs/dojo/0.7.2/bin/dojo-language-server",
  "terminal.integrated.env.osx": {
    "Q_NEW_SESSION": "1"
  },
  "update.releaseTrack": "dev",
  "update.mode": "silentlyApplyOnQuit",
  "cursor.composer.shouldChimeAfterChatFinishes": true,
  "editor.accessibilitySupport": "on",
  "git.confirmSync": false,
  "explorer.confirmDelete": false,
  "cSpell.userWords": [
    "agentic",
    "backtest",
    "bunx",
    "cooldown",
    "daiko",
    "neverthrow",
    "POLYMARKET",
    "posaune",
    "Solana",
    "Substreams",
    "timestamptz"
  ],
  "[toml]": {
    "editor.defaultFormatter": "tamasfe.even-better-toml"
  },
  "editor.fontSize": 11,
  "workbench.colorTheme": "Ayu Mirage Bordered"
}
