// Place your key bindings in this file to override the defaults
[
  {
    "key": "cmd+y",
    "command": "redo",
    "when": "textInputFocus && !editorReadonly"
  },
  {
    "key": "shift+cmd+z",
    "command": "-redo",
    "when": "textInputFocus && !editorReadonly"
  },
  {
    "key": "shift+alt+g",
    "command": "workbench.scm.focus"
  },
  {
    "key": "ctrl+cmd+g",
    "command": "cursor.generateGitCommitMessage"
  },
  {
    "key": "ctrl+cmd+s",
    "command": "git.stageAll"
  }
]
